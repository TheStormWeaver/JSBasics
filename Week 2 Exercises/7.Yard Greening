function YardGreenening(input){
    let meters = Number(input.shift())
    let ppm = 7.61
    let yard = meters * ppm
    let discount = yard * 0.18
    let pwd = yard - discount
    console.log(`The final price is: ${pwd.toFixed(2)} lv.`)
    console.log(`The discount is: ${discount.toFixed(2)} lv.`)
}
