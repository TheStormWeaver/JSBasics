function Solve(input) {
    let lengthhall = Number(input.shift())
    let widthhall = Number(input.shift())
    let wardrobe = Number(input.shift())
    let roomsize = (lengthhall * 100) * (widthhall * 100)
    let drobesize = (wardrobe*100 * wardrobe*100)
    let seatsize = roomsize / 10
    let freespace = roomsize - drobesize - seatsize
    let dancers= freespace / (40+7000)
    console.log(Math.floor(dancers))
}
