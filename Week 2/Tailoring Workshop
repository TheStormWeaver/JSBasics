function sowing(input){
    let tables = Number(input.shift());
    let tableWidth = Number(input.shift());
    let tablesHeight = Number(input.shift());
    let usd = 1.85
    let layer = tables * (tableWidth + 2 * 0.30) * (tablesHeight + 2 * 0.30)
    let check = tables * (tableWidth / 2) * (tableWidth / 2)
    let usdprice = (layer * 7) + (check * 9)
    let bgnprice = usdprice * usd
    console.log((usdprice).toFixed(2) + ' USD')
    console.log((bgnprice).toFixed(2) + ' BGN') 
}
